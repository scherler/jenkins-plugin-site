[{
    "id": "scm",
    "title": "SCM Connectors",
    "description": "Connect Jenkins to your source code manager",
    "plugins": ["bitbucket", "clearcase", "cvs", "gerrit-trigger",
      "git", "github", "gitlab-hook", "ghprb", "gitlab-plugin",
      "mercurial", "perforce", "subversion"
    ]
  }, {
    "id": "build",
    "title": "Build and Analysis",
    "description": "Select build helpers and test integration packages",
    "plugins": ["ant", "gradle", "groovy", "maven-plugin", "msbuild",
      "nodejs", "powershell", "analysis-collector", "checkstyle",
       "cobertura", "javadoc", "junit", "pmd", "sonar", "xunit"
    ]
  }, {
    "id": "deployment",
    "title": "Deployment",
    "description": "Push packages to production and track artifacts and get post build notifications",
    "plugins": ["deploy",
      "email-ext",
      "flexible-publish",
      "htmlpublisher",
      "instant-messaging",
      "jabber",
      "jira",
      "mailer",
      "publish-over-ssh",
      "redmine",
      "slack"
    ]
  }, {
    "id": "pipelines",
    "title": "Pipelines",
    "description": "Master continuous delivery and manage your complete build and deployment process",
    "plugins": ["build-pipeline-plugin", "conditional-buildstep", "copyartifact",
      "parameterized-trigger", "workflow-aggregator"]
  }, {

    "id": "containers",
    "title": "Containers",
    "description": "Distribute your process workloads and precisely package your test environments",
    "plugins": ["docker-build-publish", "docker-build-step", "docker-plugin",
      "matrix-project", "ssh-slaves", "windows-slaves"]
  }, {
    "id": "security",
    "title": "Users and Security",
    "description": "Provide fine grain user access while maintaining system security",
    "plugins": ["active-directory", "github-oauth", "ldap",
      "pam-auth", "matrix-auth"]
  }, {
    "id": "general",
    "title": "General Purpose",
    "description": "Additional tools to expidite your build automation process",
    "plugins": ["ansicolor", "build-timeout", "embeddable-build-status",
      "envinject", "next-build-number", "rebuild", "sidebar-link",
      "simple-theme-plugin", "timestamper", "translation", "ws-cleanup",
      "antisamy-markup-formatter", "cloudbees-folder", "compact-columns",
      "dashboard-view", "disk-usage", "extra-columns", "jobConfigHistory",
      "monitoring", "view-job-filters"]
  }

]
